# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Supabase Storage Configuration
SUPABASE_BUCKET_MENU_IMAGES=menu-images  # Optional, defaults to "menu-images"

# DALL-E Configuration (Optional - defaults are set in code)
# DALLE3_MAX_PER_MIN=7  # Max images per minute for DALL-E 3
# DALLE2_MAX_PER_MIN=50  # Max images per minute for DALL-E 2

# Application Configuration
PORT=8000  # Optional, defaults to 8000
ENVIRONMENT=development  # or production

# Test Mode Configuration
TEST_MODE=false  # Optional, set to true to use mock data